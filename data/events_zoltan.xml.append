<event name="ZOLTAN_FIGHT_ION">
	<text load="ZOLTAN_FIGHT_ION_TEXT"/>
	<ship load="ZOLTAN_SHIP" hostile="true"/>
	<status type="loss" target="enemy" system="shields" amount="2"/>
	<status type="loss" target="player" system="shields" amount="2"/>
		<choice hidden="true">
			<text load="GENERIC_ATTACK_TEXT"/>
