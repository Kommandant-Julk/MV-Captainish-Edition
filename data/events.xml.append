	</choice>
	<choice hidden="true" req="BEAMS_FIRE">
		<text>(Fire Beam) Offer to incinerate their crew.</text>
		<event load="QUEST_CREWDEAD_CONTINUE"/>
	</choice>
	<choice hidden="true" req="BA_BOMB_BIO">
		<text>(Anti-Personnel Bomb) Offer to dissolve their crew.</text>
		<event load="QUEST_CREWDEAD_CONTINUE"/>
	</choice>
	<choice hidden="true" req="WEAPONS_BOMB_FIRE">
		<text>(Fire Bomb) Offer to burn the crew out.</text>
		<event load="QUEST_CREWDEAD_CONTINUE"/>
	</choice>
	<choice hidden="true" req="DRONES_LIGHT">
