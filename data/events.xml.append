	<choice hidden="true">
		<text>Continue...</text>
		<event>
			<text>"My master and I are very pleased, Captain. All these rare and curious things! The cargo crew will transmit some supplies and equipment."</text>
			<autoReward level="HIGH">augment</autoReward>
			<choice hidden="true">
				<text>Continue...</text>
				<event load="CE_QUEST_CURATOR_REWARD_DRONE"/>
			</choice>
		</event>
	</choice>
</event>
